#include <stdio.h>

int potencia(int base, int expoente){
    
    for(int i=expoente; i!=1; i--){
        base=base*base;
    }
    
    
}
 
int main()
{
    int a,b;
    
    printf("Insira a base: ");
    scanf("%d", &a);
    printf("Insira o expoente: ");
    scanf("%d", &b);
    
    printf("Resultado do calculo: %d", potencia(a, b));

    return 0;
}
